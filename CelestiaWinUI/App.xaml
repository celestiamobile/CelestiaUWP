<Application
    x:Class="CelestiaWinUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:CelestiaWinUI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
            </ResourceDictionary.MergedDictionaries>
            <x:Double x:Key="HorizontalSpacing">8</x:Double>
            <x:Double x:Key="VerticalSpacing">6</x:Double>
            <x:Double x:Key="HorizontalMargin">14</x:Double>
            <x:Double x:Key="VerticalMargin">12</x:Double>
            <Thickness x:Key="Margin">14,12</Thickness>
            <Thickness x:Key="ListItemMargin">0,2</Thickness>
            <Thickness x:Key="TopSpacing">0,6,0,0</Thickness>
            <Thickness x:Key="BottomSpacing">0,0,0,6</Thickness>
            <Thickness x:Key="LeftSpacing">8,0,0,0</Thickness>
            <Thickness x:Key="RightSpacing">0,0,8,0</Thickness>
            <Thickness x:Key="TopHalf">14,12,14,6</Thickness>
            <Thickness x:Key="BottomHalf">14,6,14,12</Thickness>
        </ResourceDictionary>
    </Application.Resources>
</Application>
